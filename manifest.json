{
    "background": {
        "scripts": [
            "js/background.js",
            "libs/socket.js"
        ]
    },
    "browser_action": {
        "default_icon": "res/16.png",
        "default_popup": "popup/popup.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+Shift+V"
            }
        }
    },
    "icons": {
        "128": "res/128.png",
        "16": "res/16.png",
        "48": "res/48.png"
    },
    "manifest_version": 2,
    "name": "Hackathon",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "activeTab",
        "notifications",
        "*://*/*",
        "proxy",
        "storage",
        "alarms",
        "tabs",
        "\u003Call_urls>"
    ],
    "version": "0.1"
}
